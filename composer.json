{
    "name": "ipl/iterator-pipeline",
    "description": "Support functional-style operations, such as map-reduce transformations, on interators",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/improved-php-library/iterator-pipeline/issues",
        "source": "https://github.com/improved-php-library/iterator-pipeline"
    },
    "require": {
        "php": ">=7.1.0",
        "ipl/iterable-functions": "dev-master",
        "jasny/php-functions": "dev-master as 4.1.1"
    },
    "require-dev": {
        "jasny/php-code-quality": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Ipl\\IteratorPipeline\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ipl\\IteratorPipeline\\Tests\\": "tests/"
        }
    },
    "provides": {
        "ext-ipl_iterator_pipeline": "*"
    },
    "scripts": {
        "test": [
            "phpunit",
            "phpstan analyse",
            "phpcs -p src"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
