{
    "name": "jasny/pipeline-iterator",
    "description": "Support functional-style operations, such as map-reduce transformations, on interators",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        },
        {
            "name": "Fabio B. Silva",
            "email": "fabio.bat.silva@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/pipeline-iterator/issues",
        "source": "https://github.com/jasny/pipeline-iterator"
    },
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "jasny/php-code-quality": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\PipelineIterator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\PipelineIterator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit",
            "phpstan analyse",
            "phpcs -p src"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
