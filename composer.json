{
    "name": "jasny/iteratable-functions",
    "description": "Functions to interact with arrays, iterators and other traversable objects",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/pipeline-iterator/issues",
        "source": "https://github.com/jasny/pipeline-iterator"
    },
    "require": {
        "php": ">=7.1.0",
        "jasny/php-functions": "dev-master as 4.2.0"
    },
    "require-dev": {
        "jasny/php-code-quality": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\Iterator\\": "src/Iterator/"
        },
        "files": [
            "src/iterable_apply.php",
            "src/iterable_average.php",
            "src/iterable_cleanup.php",
            "src/iterable_concat.php",
            "src/iterable_count.php",
            "src/iterable_expect_type.php",
            "src/iterable_filter.php",
            "src/iterable_find.php",
            "src/iterable_first.php",
            "src/iterable_flatten.php",
            "src/iterable_flip.php",
            "src/iterable_group.php",
            "src/iterable_keys.php",
            "src/iterable_last.php",
            "src/iterable_map_keys.php",
            "src/iterable_map.php",
            "src/iterable_max.php",
            "src/iterable_min.php",
            "src/iterable_project.php",
            "src/iterable_reduce.php",
            "src/iterable_reverse.php",
            "src/iterable_separate.php",
            "src/iterable_sort_keys.php",
            "src/iterable_sort.php",
            "src/iterable_sum.php",
            "src/iterable_to_array.php",
            "src/iterable_to_iterator.php",
            "src/iterable_to_traversable.php",
            "src/iterable_unique.php",
            "src/iterable_values.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit",
            "phpstan analyse",
            "phpcs -p src"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
